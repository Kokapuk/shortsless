{
  "manifest_version": 3,
  "name": "Shortsless",
  "description": "Disables YouTube shorts",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["*://*.youtube.com/*"]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
